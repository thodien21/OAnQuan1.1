<Window x:Class="OAnQuan.IHM.AskDatabasePath"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OAnQuan.IHM"
        mc:Ignorable="d"
        Title="Saisissez votre chemin de base de donnée" Height="160" Width="1000">
    <StackPanel Margin="10">
        <TextBox Name="txbDBPath" Margin="20"/>
        <Button Content="OK" Width="60" Margin="10" Click="ButtonOK_Click"/>
    </StackPanel>
</Window>
